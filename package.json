{
  "name": "mya-dialog",
  "version": "0.0.1",
  "description": "Cat's dialog system",
  "main": "index.js",
  "scripts": {
    "clean:www": "rm -rf www/*",
    "clean": "npm run clean:www",
    "copy:sound": "mkdir -p www/assets/sound && cp src/sound/* www/assets/sound/",
    "copy:html": "cp src/index.html www/",
    "copy:css": "mkdir -p www/assets/css && cp src/css/reset.min.css www/assets/css/",
    "copy:img": "mkdir -p www/assets/img && cp -r src/js/p5js/data/* www/assets/img",
    "copy:development": "npm run copy:html && npm run copy:css && npm run copy:img && npm run copy:sound",
    "eslint": "`npm bin`/eslint ./src/js",
    "build": "npm run clean && npm run copy:development && `npm bin`/webpack",
    "watch:webpack": "`npm bin`/webpack --watch",
    "watch:browser": "`npm bin`/browser-sync start --config bs-config.js",
    "watch": "npm run watch:webpack & npm run watch:browser",
    "start": "npm run build && npm run watch"
  },
  "author": "Naoki Sawada",
  "license": "MIT",
  "dependencies": {
    "p5": "^0.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.6.1",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "~1.12.12"
  }
}
